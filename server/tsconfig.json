{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "../build",
    "rootDir": "./src",
    "strict": true,
    "alwaysStrict": true,

    "esModuleInterop": true,

    "experimentalDecorators": true,

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
